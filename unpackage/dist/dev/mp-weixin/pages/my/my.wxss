.header {
  background: #fff;
  height: 290rpx;
  padding-bottom: 110rpx;
}
.header .bg {
  width: 100%;
  height: 270rpx;
  padding-top: 100rpx;
  background-color: #6A5DAC;
}
.box {
  width: 650rpx;
  height: 280rpx;
  border-radius: 20rpx;
  margin: auto;
  background: #fff;
  box-shadow: 0 5rpx 20rpx 0rpx rgba(0, 0, 150, 0.2);
}
.box .box-hd {
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  justify-content: center;
}
.box .box-hd .avator {
  width: 160rpx;
  height: 160rpx;
  background: #fff;
  border: 5rpx solid #fff;
  border-radius: 50%;
  margin-top: -80rpx;
  overflow: hidden;
}
.box .box-hd .phone-number {
  width: 100%;
  text-align: center;
}
.box .box-bd {
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  justify-content: center;
}
.box .box-bd .item {
  flex: 1 1 auto;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  justify-content: center;
  border-right: 1px solid #f1f1f1;
  margin: 15rpx 0;
}
.box .box-bd .item:last-child {
  border: none;
}
.box .box-bd .item .icon {
  width: 60rpx;
  height: 60rpx;
}
.box .box-bd .item .icon ._img {
  width: 100%;
  height: 100%;
}
.box .box-bd .item .text {
  width: 100%;
  text-align: center;
  margin-top: 10rpx;
}
.list-content {
  background: #fff;
}
.list {
  width: 100%;
  border-bottom: 15rpx solid #f1f1f1;
  background: #fff;
}
.list:last-child {
  border: none;
}
.list .li {
  width: 92%;
  height: 100rpx;
  padding: 0 4%;
  border-bottom: 1px solid #f3f3f3;
  display: flex;
  align-items: center;
}
.list .li.noborder {
  border-bottom: 0;
}
.list .li .icon {
  flex-shrink: 0;
  width: 50rpx;
  height: 50rpx;
}
.list .li .icon image {
  width: 50rpx;
  height: 50rpx;
}
.list .li .text {
  padding-left: 20rpx;
  width: 100%;
  color: #666;
}
.list .li .to {
  flex-shrink: 0;
  width: 40rpx;
  height: 40rpx;
}
.content {
  width: 100%;
  box-sizing: border-box;
}
.content .topView {
  width: auto;
  height: 90rpx;
  display: flex;
  margin-bottom: 1rpx;
  padding: 10rpx;
  background-color: #fff;
}
.content .topView .topL {
  width: 80rpx;
  height: 100%;
}
.content .topView .topL image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
}
.content .topView .topC {
  max-width: 320rpx;
  height: auto;
  margin: 5rpx 5rpx 5rpx 10rpx;
  display: flex;
  flex-flow: row wrap;
  align-content: space-between;
}
.content .topView .topC .title {
  width: 100%;
  height: 50%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 32rpx;
  color: #333;
}
.content .topView .topC .status {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 27rpx;
  color: #333;
}
.content .topView .topR {
  height: 100%;
  flex: 1;
  display: flex;
  align-items: center;
  flex-flow: row-reverse;
}
.content .topView .attention {
  height: 60rpx;
  line-height: 60rpx;
  width: 110rpx;
  text-align: center;
  font-size: 27rpx;
  background-color: #13b6ff;
  border-radius: 30rpx;
  color: #fff;
}
.content .courseVideo {
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.content .courseVideo .videoView {
  margin: 15rpx;
  width: 100%;
  height: 300rpx;
  box-sizing: border-box;
  background-color: #fff;
  display: flex;
}
.content .courseVideo .videoView .video-container {
